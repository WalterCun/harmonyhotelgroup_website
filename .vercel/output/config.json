{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/404/?$",
			"dest": "_render"
		},
		{
			"src": "^/500/?$",
			"dest": "_render"
		},
		{
			"src": "^/blog/?$",
			"dest": "_render"
		},
		{
			"src": "^/contact/?$",
			"dest": "_render"
		},
		{
			"src": "^/destinations/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/destinations/?$",
			"dest": "_render"
		},
		{
			"src": "^/en/404/?$",
			"dest": "_render"
		},
		{
			"src": "^/en/500/?$",
			"dest": "_render"
		},
		{
			"src": "^/en/blog/?$",
			"dest": "_render"
		},
		{
			"src": "^/en/contact/?$",
			"dest": "_render"
		},
		{
			"src": "^/en/destinations/?$",
			"dest": "_render"
		},
		{
			"src": "^/en/hotels/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/en/hotels/?$",
			"dest": "_render"
		},
		{
			"src": "^/en/login/?$",
			"dest": "_render"
		},
		{
			"src": "^/en/offers/?$",
			"dest": "_render"
		},
		{
			"src": "^/en/?$",
			"dest": "_render"
		},
		{
			"src": "^/hotels/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/hotels/?$",
			"dest": "_render"
		},
		{
			"src": "^/login/?$",
			"dest": "_render"
		},
		{
			"src": "^/offers/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		},
		{
			"src": "^/.*$",
			"dest": "_render",
			"status": 404
		}
	]
}