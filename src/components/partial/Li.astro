---
export const partial = true;

interface Props {
    href: string
    label: string
}

const {
    href,
    label
} = Astro.props;
---
<li>
    <a href={href}
       class="text-neutral-700 dark:text-neutral-300 hover:text-primary-500 dark:hover:text-primary-400 font-medium transition-colors duration-200">
        {label}
    </a>
</li>