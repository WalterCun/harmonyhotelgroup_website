{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*",
    "src/**/*",
    "astro.config.mjs",
    "tailwind.config.mjs"
  ],
  "exclude": [
    "node_modules",
    "dist",
    ".vercel",
    ".vscode"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "src/*"
      ],
      "@api": [
        "src/utils/*"
      ]
    }
  }
}
