{
	"extends": "astro/tsconfigs/strict",
	"include": [
		".astro/types.d.ts",
		"**/*",
		"src/**/*",
		"astro.config.mjs",
		"tailwind.config.mjs"
	],
	"exclude": ["node_modules", "dist", ".vercel", ".vscode",".astro",".github",".idea","tmp"],
	"compilerOptions": {
		"baseUrl": "./",
		"paths": {
			"~/*": ["src/*"],
			"lib/*": ["src/lib/*"]
		}
	}
}
